local Library = loadstring(game:HttpGet("https://raw.githubusercontent.com/xHeptc/Kavo-UI-Library/main/source.lua"))()
local Window = Library.CreateLib("BLOODBANDER FE", "Sentinel")

--Tabs Cframe Speed / Jump
local Tab = Window:NewTab("Speed/Jump")
local Section = Tab:NewSection("Speed/Jump")


--Cframe Speed / Jump
Section:NewSlider("Speed", "Gives you Speed", 1000, 16, function(s) -- 500 (MaxValue) | 0 (MinValue)
    game.Players.LocalPlayer.Character.Humanoid.WadlkSpeed = s
    end)

    Section:NewSlider("JumpPower", "You Cn Jump Higher", 500, 50, function(s) -- 500 (MaxValue) | 0 (MinValue)
    game.Players.LocalPlayer.Character.Humanoid.JumpPower = s
end)
--tabs Fun
