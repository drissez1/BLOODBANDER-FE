end)

Section:NewToggle("Noclip", "You Can Walk On Walls", function(state)
    if state then
        loadstring(game:HttpGet(('https://raw.githubusercontent.com/drissez1/Noclip/main/Noclip'),true))()
    else
        loadstring(game:HttpGet(('https://raw.githubusercontent.com/drissez1/Noclip/main/Noclip'),true))()
    end
end)
