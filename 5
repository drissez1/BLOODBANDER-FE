local Library = loadstring(game:HttpGet("https://raw.githubusercontent.com/xHeptc/Kavo-UI-Library/main/source.lua"))()
local Window = Library.CreateLib("BloodBander Chooser V1(Everything Works With Solara)", "DarkTheme")

local Tab = Window:NewTab("Our Versions")
local Section = Tab:NewSection("Our Versions")
Section:NewButton("Universal BloodBander V1.0", "Our Universal Script", function()
    loadstring(game:HttpGet("https://raw.githubusercontent.com/drissez1/BLOODBANDER-FE/main/FE"))()
end)

--Da Hood Tab
local Tab = Window:NewTab("Da Hood")
local Section = Tab:NewSection("Da Hood Script")
Section:NewButton("Faded Lastest Version(V2.4)", "Faded Scirpt Da Hood", function()
    loadstring(game:HttpGet("https://raw.githubusercontent.com/NighterEpic/Faded/main/YesEpic", true))()
end)
Section:NewButton("Best CamLock", "CamLock For Da Hood", function()
    loadstring(game:HttpGet("https://raw.githubusercontent.com/drissez1/Lock/main/Lock", true))()
end)
Section:NewButton("Azure Modded Paid Version (V2.0)", "Azure Modded Paid Version", function()
    loadstring(game:HttpGet("https://raw.githubusercontent.com/Actyrn/Scripts/main/AzureModded"))()
end)
Section:NewButton("Best Anti Lock UNREASOLVABLE", "Best AntiLock", function()
    getgenv().Sky = true --disable or enable the antilock
getgenv().SkyAmount = 100 --how far it should be
 
game:GetService("RunService").heartbeat:Connect(function()
    if getgenv().Sky ~= false then 
    local vel = game.Players.LocalPlayer.Character.HumanoidRootPart.Velocity
    game.Players.LocalPlayer.Character.HumanoidRootPart.Velocity = Vector3.new(0,      getgenv().SkyAmount,0) 
    game:GetService("RunService").RenderStepped:Wait()
    game.Players.LocalPlayer.Character.HumanoidRootPart.Velocity = vel
    end 
end)
end)

--BloxFruit Tab
local Tab = Window:NewTab("BloxFruit")
local Section = Tab:NewSection("Scripts For BloxFruits")
Section:NewButton("Matsune Lastest Version (V1.8)", "MatSune Script BloxFruit", function()
    loadstring(game:HttpGet("https://raw.githubusercontent.com/Yatsuraa/Yuri/main/Winterhub_V2.lua"))()
end)
